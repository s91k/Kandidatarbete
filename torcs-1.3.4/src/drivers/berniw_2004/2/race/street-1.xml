<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="car5-trb1" type="template">

	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.59"/>
		<attnum name="max pressure" min="100" max="150000" unit="kPa" val="30000"/>
	</section>

	<section name="Front Wing">
		<attnum name="angle" min="0" max="11" unit="deg" val="9"/>
	</section>

	<section name="Rear Wing">
		<attnum name="angle" min="0" max="16" unit="deg" val="13"/>
	</section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-2.0"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="2.66"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="1.68"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.3"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.1"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="0.97"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="0.84"/>
      </section>
    </section>
  </section>

	<section name="berniw private">
		<attnum name="caero" val="1.2"/>
		<attnum name="cfriction" val="1.2"/>
		<attnum name="fuelperlap" val="2.6"/>
		<attnum name="steerpgain" val="0.12"/>
		<attnum name="steerpgainmax" val="0.4"/>
		<attstr name="teammate" val="berniw 2004 1"/>
	</section>
	
</params>
