<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="car4-trb1" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="12" val="12"/>
	</section>
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="0" max="17" val="17"/>
	</section>
	<section name="Gearbox">
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-3" max="0" val="-2.0"/>
			</section>
			<section name="1">
				<attnum name="ratio" min="0" max="5" val="3.8"/>
			</section>
			<section name="2">
				<attnum name="ratio" min="0" max="5" val="2.8"/>
			</section>
			<section name="3">
				<attnum name="ratio" min="0" max="5" val="1.85"/>
			</section>
			<section name="4">
				<attnum name="ratio" min="0" max="5" val="1.45"/>
			</section>
			<section name="5">
				<attnum name="ratio" min="0" max="5" val="1.25"/>
			</section>
			<section name="6">
				<attnum name="ratio" min="0" max="5" val="1.08"/>
			</section>
		</section>
	</section>
	<section name="Rear Differential">
		<attstr name="type" in="SPOOL,FREE, LIMITED SLIP" val="LIMITED SLIP"/>
		<attnum name="ratio" min="0" max="10" val="3.07"/>
	</section>
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.60"/>
		<attnum name="max pressure" unit="kPa" min="100" max="150000" val="40000"/>
	</section>
	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="70" max="120" val="70"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="-0.4"/>
		<attnum name="camber" min="-5" max="-3" unit="deg" val="-5"/>
	</section>
	
	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="70" max="120" val="70"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0.4"/>
		<attnum name="camber" min="-5" max="-3" unit="deg" val="-5"/>
	</section>
	
	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="70" max="120" val="70"/>
		<attnum name="toe" unit="deg" min="0" max="0" val="0"/>
		<attnum name="camber" min="-5" max="-2" unit="deg" val="-5"/>
	</section>
	
	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="70" max="120" val="70"/>
		<attnum name="toe" unit="deg" min="0" max="0" val="0"/>
		<attnum name="camber" min="-5" max="-2" unit="deg" val="-5"/>
	</section>
	
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>
	
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="2500"/>
	</section>

	<section name="Front Right Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="3600"/>
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.2"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="7"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="180"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="240"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="30"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="40"/>
	</section>
	
	<section name="Front Left Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="3600"/>
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.2"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="7"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="180"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="240"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="30"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="40"/>
	</section>
	
	<section name="Rear Right Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="4000"/>
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.2"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="7"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="180"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="240"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="30"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="40"/>
	</section>
	
	<section name="Rear Left Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="4000"/>
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.2"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.2"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="7"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="1000" val="180"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="1000" val="240"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="1000" val="30"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="1000" val="40"/>
	</section>
	
	<section name="wdbee private">
		<attnum name="max fuel"  min="0" max="95" val="95"/>

		<attnum name="aero mod" min="0" max="2" val="2"/>
		<attnum name="bump mod" min="0.0" max="5.0" val="1"/>

		<attnum name="border inner" min="-5" max="+5" val="0.00"/>
		<attnum name="border outer" min="-5" max="+5" val="0.00"/>
		<attnum name="border inner max" min="-5" max="+5" val="0.31"/>
		<attnum name="border scale" min="0" max="500" val="120.0"/>

		<attnum name="clutch delta" min="0" max="0.99" val="0.05"/>
		<attnum name="clutch max" min="0" max="0.99" val="0.5"/>
		<attnum name="clutch range" min="0" max="0.99"  val="0.82"/>
		<attnum name="clutch release" min="0" max="0.99"  val="0.345"/>

		<attnum name="filter drifting" min="0" max="1" val="1"/>
		<attnum name="filter track" min="0" max="1" val="1"/>
		<attnum name="filter tcl" min="0" max="1" val="1"/>

		<attnum name="fuelper100km" min="0.0" max="100.0" val="70.0"/>
		<attnum name="scale refuel" min="1.00" max="1.20" val="1.10"/>

		<attnum name="lookaheadfactor" min="0" max="0.5" val="0.05"/>
		<attnum name="max damage" min="1" max="9999" val="5000"/>
		<attnum name="omegaaheadfactor" min="0" max="5" val="0.1"/>

		<attnum name="start side" min="-1" max="1" val="1.0"/>
		<attnum name="length margin" min="0" max="3" val="2.0"/>

		<attnum name="save plot" min="0" max="1" val="0"/>
		<attnum name="save stat" min="0" max="1" val="0"/>
		<attnum name="save sect" min="0" max="1" val="0"/>
		<attnum name="show index" min="0" max="1" val="0"/>

		<attnum name="pit test stop" min="0" max="1" val="0"/>
		<attnum name="pit mu scale" min="0" max="1.0" val="1.0"/>

		<attnum name="pit entry long" min="-2000" max="2000" val="0.0"/>
		<attnum name="pit exit long" min="-2000" max="2000" val="0"/>
		<attnum name="pit exit length" min="0" max="2000" val="0.0"/>
		<attnum name="pit lat offset" min="-2" max="10" val="0.0"/>
		<attnum name="pit long offset" min="-2" max="10" val="0.0"/>
		<attnum name="pit stop dist" min="0" max="10" val="1.5"/>
		<attnum name="pit use first" min="0" max="1" val="1"/>
		<attnum name="pit use smooth" min="0" max="1" val="1"/>

		<attnum name="pitlane entry offset" min="-10" max="10" val="0.5"/>
		<attnum name="pitlane exit offset" min="-10" max="10" val="1.0"/>

		<attnum name="qualification" min="0" max="1" val="0"/>
		<attnum name="reserve" min="0" max="20000" val="2000"/>

		<attnum name="avoid width" min="1.0" max="20.0" val="0.5"/>
		<attnum name="scale avoid" min="1.0" max="20.0" val="7.5"/>

		<attnum name="fly level" min="0.0" max="1.0" val="0.031"/>
		<attnum name="fly section" val="0"/>
		<attnum name="max crvangle" val="9.0"/>
		<attnum name="min scale mu" min="0" max="1" val="0.97"/>

		<attnum name="initial brake" min="0" max="1" val="0.75"/>
		<attnum name="scale brake" min="0" max="5" val="0.92"/>
		<attnum name="scale bump" min="0" max="2" val="0.3"/>
		<attnum name="scale bump outer" min="0" max="2" val="0.6"/>
		<attnum name="scale min mu" min="0" max="2" val="0.8"/>
		<attnum name="scale mu" min="0" max="5" val="0.98"/>

		<attnum name="scale steer" min="0.0" max="2.0" val="0.8"/>
		<attnum name="stay together" min="0.0" max="10000.0" val="10"/>

		<attnum name="skill" min="-1" max="2" val="-1"/>
	</section>
</params>

