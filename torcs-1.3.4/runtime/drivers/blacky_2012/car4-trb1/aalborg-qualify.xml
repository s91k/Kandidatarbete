    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="4164"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.12"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="5"/>
