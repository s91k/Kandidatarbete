<!DOCTYPE params SYSTEM "params.dtd">


<params name="car4-trb1">
  <section name="Front Wing">
    <attnum name="angle" min="0" max="12" unit="deg" val="11.07"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" min="0" max="17" unit="deg" val="14.89"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-2"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="3.8"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="2.8"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.85"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.45"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="1.25"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="1.08"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attstr name="type" in="SPOOL,FREE, LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" min="0" max="10" val="4.187"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.5" max="0.7" val="0.5771"/>
