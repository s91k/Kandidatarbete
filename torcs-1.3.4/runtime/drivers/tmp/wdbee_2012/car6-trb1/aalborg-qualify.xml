    <attnum name="length margin" min="0" max="10" val="2"/>

    <attnum name="save plot" min="0" max="1" val="0"/>
    <attnum name="save stat" min="0" max="1" val="0"/>
    <attnum name="save sect" min="0" max="1" val="0"/>
    <attnum name="show index" min="0" max="1" val="0"/>

    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="1"/>

    <attnum name="avoid width" min="0" max="20" val="0"/>
    <attnum name="scale avoid" min="1" max="20" val="12"/>

    <attnum name="min scale mu" min="0" max="1" val="0.97"/>
    <attnum name="min scale brake" min="0" max="1" val="0.95"/>

    <attnum name="initial brake" min="0" max="1" val="1"/>
