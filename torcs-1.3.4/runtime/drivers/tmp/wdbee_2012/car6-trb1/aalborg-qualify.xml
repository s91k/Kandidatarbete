<!DOCTYPE params SYSTEM "params.dtd">


<params name="car6-trb1">
  <section name="Front Wing">
    <attnum name="angle" min="0" max="12" unit="deg" val="11.3"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" min="0" max="17" unit="deg" val="16.98"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-3"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="5" val="3.28"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="2.44"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="1.7"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.335"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="1.195"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="1"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attstr name="type" in="SPOOL,LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" min="0" max="10" val="3.81"/>
  </section>

  <section name="Brake System">
    <attnum name="_front-rear brake repartition" min="0.5" max="0.55" val="0.544"/>
