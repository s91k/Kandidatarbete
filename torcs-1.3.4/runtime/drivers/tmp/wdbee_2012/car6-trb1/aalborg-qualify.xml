    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.12"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="143"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="300"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="48"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="27"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="3977"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.12"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="143"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="300"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="48"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="27"/>
  </section>

