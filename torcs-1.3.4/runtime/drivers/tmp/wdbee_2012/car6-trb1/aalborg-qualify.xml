    <attnum name="max fuel" min="0" max="91" val="91"/>
    <attnum name="start fuel" min="-1" max="91" val="-1"/>

    <attnum name="filter drifting" min="0" max="1" val="1"/>
    <attnum name="filter track" min="0" max="1" val="1"/>
    <attnum name="filter tcl" min="0" max="1" val="1"/>

    <attnum name="_lookaheadfactor" min="0" max="0.08" val="0.08"/>
